<h3>Contact</h3>
<div class="card z-depth-3">
	<div class="card-content">
		<p>Do you wanna talk? Find me on the map and we can have some drinks</p>
		<br/>
		<div id="map-canvas"></div>
	</div>
</div>

<style>
#map-canvas {
	width: 100%;
	height: 400px;
	background-color: grey;
}
</style>
<script async defer   src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCkV58mEQU-6s06lkB056Yh29OpIwbYuNE&callback=initMap">
</script>

<script>
	function initMap() {
		var uluru = {lat: -37.81, lng: 144.96};
		var map = new google.maps.Map(document.getElementById('map-canvas'), {
			zoom: 14,
			center: uluru
		});
		var marker = new google.maps.Marker({
			position: uluru,
			map: map,
			title: 'This is Melburrone!' 
		});

		marker.setMap(map);
	}
</script>

